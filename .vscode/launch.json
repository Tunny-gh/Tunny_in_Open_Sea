  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Rhino8",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "C:/Program Files/Rhino 8/System/Rhino.exe",
      "args": ["/nosplash"],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": true
    },
    {
      "name": "Run Rhino7",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "C:/Program Files/Rhino 7/System/Rhino.exe",
      "args": ["/nosplash"],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": true
    },
    {
      "name": "Run Revit2025",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "C:/Program Files/Autodesk/Revit 2025/Revit.exe",
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": true
    },
  ]
}